<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="default" namespace="/" extends="struts-default">

        <!-- 定义一个action -->
        <action name="login" class="com.gkpoter.dazuoye.action.LoginAction" method="Login">
        </action>
        <action name="home" class="com.gkpoter.dazuoye.action.STHomeAction" method="getHome">
        </action>
        <action name="upload" class="com.gkpoter.dazuoye.action.UpLoadAction" method="upLoadFile">
        </action>
        <action name="chooseclass" class="com.gkpoter.dazuoye.action.ChooseClassAction" method="ChooseClass">
        </action>
        <action name="listclass" class="com.gkpoter.dazuoye.action.ListClassAction" method="ListClass">
        </action>
        <action name="videolog" class="com.gkpoter.dazuoye.action.VideoLogAction" method="videoLog">
        </action>
    </package>
</struts>